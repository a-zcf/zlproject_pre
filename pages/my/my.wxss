.my{
  width: 100%;
}
.my > .my-head{
  width: 100%;
  height: 200rpx;
  background-color: #18b5cd;
  display: flex;
  align-items: center;
}
.my > .my-head > .avatar{
width: 150rpx;
height: 150rpx;
border-radius: 100rpx;
margin: 0rpx 50rpx 0rpx 50rpx;
}
.my-head > .nickname > .phone{
   margin-bottom: 15rpx;
}
.my-list{
  width: 100%;
  margin-top: 25rpx;
}
.my-list > .list-con{
  width: 95%;
  margin: auto;
  margin-top: 15rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid rgb(201, 201, 201);
  padding-bottom: 15rpx;
}
.my-list > .list-con > .icomImg{
  width: 90rpx;
  height: 90rpx;
  margin-right: 30rpx;
}
.list-con > .listText{
  margin-right: 50rpx;
  font-size: 35rpx;
}
.list-con > .icon-more{
  margin-left: 330rpx;
}